<https://example.org/plugins/abÃ¨switch>
	a doap:Project ;
	doap:maintainer <https://github.com/ycollet#me> ;
	doap:name "ab-switch" .

<http://example.org/plugins/ab-switch>
    a lv2:Plugin ;
    doap:name "AB Switch (MIDI controlled)" ;
    lv2:port [
        a lv2:InputPort , lv2:AudioPort ;
        lv2:index 0 ; lv2:symbol "in_a_l" ; lv2:name "In A Left"
    ] , [
        a lv2:InputPort , lv2:AudioPort ;
        lv2:index 1 ; lv2:symbol "in_a_r" ; lv2:name "In A Right"
    ] , [
        a lv2:InputPort , lv2:AudioPort ;
        lv2:index 2 ; lv2:symbol "in_b_l" ; lv2:name "In B Left"
    ] , [
        a lv2:InputPort , lv2:AudioPort ;
        lv2:index 3 ; lv2:symbol "in_b_r" ; lv2:name "In B Right"
    ] , [
        a lv2:OutputPort , lv2:AudioPort ;
        lv2:index 4 ; lv2:symbol "out_l_a" ; lv2:name "Out A Left"
    ] , [
        a lv2:OutputPort , lv2:AudioPort ;
        lv2:index 5 ; lv2:symbol "out_r_a" ; lv2:name "Out A Right"
    ] , [
        a lv2:OutputPort , lv2:AudioPort ;
        lv2:index 6 ; lv2:symbol "out_l_b" ; lv2:name "Out B Left"
    ] , [
        a lv2:OutputPort , lv2:AudioPort ;
        lv2:index 7 ; lv2:symbol "out_r_b" ; lv2:name "Out B Right"
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "ccNumber" ;
        lv2:name "MIDI CC Number" ;
        lv2:default 64 ;
        lv2:minimum 0 ;
        lv2:maximum 127
    ] .
